{
  "name": "@seleniumhq/selenium-ide",
  "version": "4.0.1",
  "private": true,
  "description": "Selenium IDE electron app",
  "author": "Tomer <tomer@corevo.io>",
  "homepage": "http://github.com/SeleniumHQ/selenium-ide",
  "license": "Apache-2.0",
  "scripts": {
    "copy:tsconfig": "cp ./tsconfig.json ./dist/tsconfig.json",
    "start": "electron dist/main-bundle.js",
    "build": "run-s build:chromedriver build:webpack build:electron",
    "build:chromedriver": "mkdir -p files && node scripts/download-driver.js",
    "build:electron": "electron-builder -c.electronVersion $(node -e \"console.log(require('electron/package.json').version)\") --publish never",
    "build:webpack": "webpack",
    "watch": "webpack --watch"
  },
  "sideEffects": false,
  "main": "dist/core/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeleniumHQ/selenium-ide.git"
  },
  "bugs": {
    "url": "https://github.com/SeleniumHQ/selenium-ide/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "build": {
    "appId": "org.seleniumhq.selenium-ide",
    "productName": "Selenium IDE",
    "asarUnpack": "files/*",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools",
      "darkModeSupport": true
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "fileAssociations": [
      {
        "ext": "side",
        "name": "Selenium IDE 3 project",
        "role": "Editor"
      },
      {
        "ext": "sideproj",
        "name": "Selenium IDE project",
        "role": "Editor"
      }
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^5.0.0-beta.1",
    "@material-ui/icons": "^5.0.0-beta.1",
    "@material-ui/styled-engine-sc": "^5.0.0-beta.1",
    "@seleniumhq/get-driver": "^3.5.10",
    "@seleniumhq/side-model": "^3.5.10",
    "@seleniumhq/side-runtime": "^3.5.10",
    "electron": "^16.0.5",
    "electron-store": "^8.0.0",
    "fs-extra": "^8.1.0",
    "immer": "9.0.7",
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "selenium-webdriver": "^4.1.0",
    "spectron": "15.0.0",
    "use-immer": "0.6.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.57",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/electron": "1.6.10",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "4.14.170",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-env": "1.16.0",
    "css-loader": "^6.1.0",
    "electron-builder": "^22.10.5",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.1.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "5.37.0"
  }
}
